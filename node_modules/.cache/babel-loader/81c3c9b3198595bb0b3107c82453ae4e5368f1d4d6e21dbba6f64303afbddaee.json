{"ast":null,"code":"import HomePanel from './home-panel';\nimport { ref } from 'vue';\nimport { findBrand } from '@/api/home';\nexport default {\n  name: 'HomeBrand',\n  components: {\n    HomePanel\n  },\n  setup() {\n    // 获取数据\n    const brands = ref([]);\n    findBrand(10).then(data => {\n      brands.value = data.result;\n    });\n    // 切换效果\n    const index = ref(0);\n    // 点击上一页\n    // 点击下一页\n    const toggle = step => {\n      const newIndex = index.value + step;\n      if (newIndex < 0 || newIndex > 1) {\n        index.value = newIndex;\n      }\n    };\n    return {\n      brands\n    };\n  }\n};","map":{"version":3,"mappings":"AA6BA,OAAOA,SAAQ,MAAO,cAAc;AACpC,SAASC,GAAE,QAAS,KAAK;AACzB,SAASC,SAAQ,QAAS,YAAY;AACtC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAAEJ;EAAU,CAAC;EACzBK,KAAK,GAAG;IACN;IACA,MAAMC,MAAK,GAAIL,GAAG,CAAC,EAAE,CAAC;IACtBC,SAAS,CAAC,EAAE,CAAC,CAACK,IAAI,CAAEC,IAAI,IAAK;MAC3BF,MAAM,CAACG,KAAI,GAAID,IAAI,CAACE,MAAM;IAC5B,CAAC,CAAC;IACF;IACA,MAAMC,KAAI,GAAIV,GAAG,CAAC,CAAC,CAAC;IACpB;IACA;IACA,MAAMW,MAAK,GAAKC,IAAI,IAAK;MACvB,MAAMC,QAAO,GAAIH,KAAK,CAACF,KAAI,GAAII,IAAI;MACnC,IAAIC,QAAO,GAAI,KAAKA,QAAO,GAAI,CAAC,EAAE;QAChCH,KAAK,CAACF,KAAI,GAAIK,QAAQ;MACxB;IACF,CAAC;IACD,OAAO;MAAER;IAAS,CAAC;EACrB;AACF,CAAC","names":["HomePanel","ref","findBrand","name","components","setup","brands","then","data","value","result","index","toggle","step","newIndex"],"sourceRoot":"","sources":["E:\\自己写的项目\\tuxian\\erabbit-pc-vue-project\\src\\views\\home\\components\\home-brand.vue"],"sourcesContent":["<template>\n  <HomePanel title=\"热门品牌\" sub-title=\"国际经典 品质保证\">\n    <template v-slot:right>\n      <a\n        @click=\"toggle(-1)\"\n        :class=\"{ disabled: index === 0 }\"\n        href=\"javascript:;\"\n        class=\"iconfont icon-angle-left prev\"\n      ></a>\n      <a\n        @click=\"toggle(1)\"\n        :class=\"{ disabled: index === 1 }\"\n        href=\"javascript:;\"\n        class=\"iconfont icon-angle-right next\"\n      ></a>\n    </template>\n    <div class=\"box\" ref=\"box\">\n      <ul class=\"list\">\n        <li v-for=\"item in brands\" :key=\"item.id\">\n          <RouterLink to=\"/\">\n            <img :src=\"item.picture\" alt=\"\" />\n          </RouterLink>\n        </li>\n      </ul>\n    </div>\n  </HomePanel>\n</template>\n\n<script>\nimport HomePanel from './home-panel';\nimport { ref } from 'vue';\nimport { findBrand } from '@/api/home';\nexport default {\n  name: 'HomeBrand',\n  components: { HomePanel },\n  setup() {\n    // 获取数据\n    const brands = ref([]);\n    findBrand(10).then((data) => {\n      brands.value = data.result;\n    });\n    // 切换效果\n    const index = ref(0);\n    // 点击上一页\n    // 点击下一页\n    const toggle = (step) => {\n      const newIndex = index.value + step;\n      if (newIndex < 0 || newIndex > 1) {\n        index.value = newIndex;\n      }\n    };\n    return { brands,  };\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n.home-panel {\n  background: #f5f5f5;\n}\n.iconfont {\n  width: 20px;\n  height: 20px;\n  background: #ccc;\n  color: #fff;\n  display: inline-block;\n  text-align: center;\n  margin-left: 5px;\n  background: @xtxColor;\n  &::before {\n    font-size: 12px;\n    position: relative;\n    top: -2px;\n  }\n  &.disabled {\n    background: #ccc;\n    cursor: not-allowed;\n  }\n}\n.box {\n  display: flex;\n  width: 100%;\n  height: 345px;\n  overflow: hidden;\n  padding-bottom: 40px;\n  .list {\n    width: 200%;\n    display: flex;\n    transition: all 1s;\n    li {\n      margin-right: 10px;\n      width: 240px;\n      &:nth-child(5n) {\n        margin-right: 0;\n      }\n      img {\n        width: 240px;\n        height: 305px;\n      }\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}