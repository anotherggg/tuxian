{"ast":null,"code":"import { useStore } from 'vuex';\nimport { computed } from 'vue';\nexport default {\n  name: 'AppTopnav',\n  setup() {\n    const store = useStore;\n    const profile = computed(() => {\n      return store.state.user.profile;\n    });\n    return profile;\n  }\n};","map":{"version":3,"mappings":"AA2BA,SAASA,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAO,QAAS,KAAK;AAC9B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ;IACtB,MAAMK,OAAM,GAAIJ,QAAQ,CAAC,MAAM;MAC7B,OAAOG,KAAK,CAACE,KAAK,CAACC,IAAI,CAACF,OAAO;IACjC,CAAC,CAAC;IACF,OAAOA,OAAO;EAChB;AACF,CAAC","names":["useStore","computed","name","setup","store","profile","state","user"],"sourceRoot":"","sources":["E:\\自己写的项目\\tuxian\\erabbit-pc-vue-project\\src\\components\\app-topnav.vue"],"sourcesContent":["<template>\n  <nav class=\"app-topnav\">\n    <div class=\"container\">\n      <ul>\n        <template v-if=\"profile.token\">\n          <li>\n            <a href=\"javascript:;\"><i class=\"iconfont icon-user\"></i>{{profile}}</a>\n          </li>\n          <li><a href=\"javascript:;\">退出登录</a></li>\n        </template>\n        <template>\n        <li><a href=\"javascript:;\">请先登录</a></li>\n        <li><a href=\"javascript:;\">免费注册</a></li>\n        </template>\n        <li><a href=\"javascript:;\">我的订单</a></li>\n        <li><a href=\"javascript:;\">会员中心</a></li>\n        <li><a href=\"javascript:;\">帮助中心</a></li>\n        <li><a href=\"javascript:;\">关于我们</a></li>\n        <li>\n          <a href=\"javascript:;\"><i class=\"iconfont icon-phone\"></i>手机版</a>\n        </li>\n      </ul>\n    </div>\n  </nav>\n</template>\n\n<script>\nimport { useStore } from 'vuex';\nimport { computed } from 'vue';\nexport default {\n  name: 'AppTopnav',\n  setup() {\n    const store = useStore;\n    const profile = computed(() => {\n      return store.state.user.profile;\n    });\n    return profile;\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n.app-topnav {\n  background: #333;\n  ul {\n    display: flex;\n    height: 53px;\n    justify-content: flex-end;\n    align-items: center;\n    li {\n      a {\n        padding: 0 15px;\n        color: #cdcdcd;\n        line-height: 1;\n        display: inline-block;\n        i {\n          font-size: 14px;\n          margin-right: 2px;\n        }\n        &:hover {\n          color: @xtxColor;\n        }\n      }\n      ~ li {\n        a {\n          border-left: 2px solid #666;\n        }\n      }\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}