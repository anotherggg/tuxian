{"ast":null,"code":"import { useStore } from 'vuex';\nimport { computed } from 'vue';\nexport default {\n  name: 'AppHeaderNav',\n  setup() {\n    const store = useStore();\n    const list = computed(() => {\n      return store.state.category.list;\n    });\n    const show = item => {\n      store.commit();\n    };\n    return {\n      list\n    };\n  }\n};","map":{"version":3,"mappings":"AAoBA,SAASA,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAO,QAAS,KAAK;AAC9B,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,EAAE;IACxB,MAAMK,IAAG,GAAIJ,QAAQ,CAAC,MAAM;MAC1B,OAAOG,KAAK,CAACE,KAAK,CAACC,QAAQ,CAACF,IAAI;IAClC,CAAC,CAAC;IACF,MAAMG,IAAG,GAAKC,IAAI,IAAK;MACrBL,KAAK,CAACM,MAAM,EAAC;IACf;IACA,OAAO;MAAEL;IAAK,CAAC;EACjB;AACF,CAAC","names":["useStore","computed","name","setup","store","list","state","category","show","item","commit"],"sourceRoot":"","sources":["E:\\自己写的项目\\tuxian\\erabbit-pc-vue-project\\src\\components\\app-header-nav.vue"],"sourcesContent":["<template>\n  <ul class=\"app-header-nav\">\n    <li class=\"home\"><RouterLink to=\"/\">首页</RouterLink></li>\n    <li v-for=\"item in list\" :key=\"item.id\">\n      <router-link to=\"/\">{{ item.name }}</router-link>\n      <div class=\"layer\">\n        <ul>\n          <li v-for=\"sub in item.children\" :key=\"sub.id\">\n            <router-link to=\"/\">\n              <img :src=\"sub.picture\" alt=\"/\" />\n              <p>{{ sub.name }}</p>\n            </router-link>\n          </li>\n        </ul>\n      </div>\n    </li>\n  </ul>\n</template>\n\n<script>\nimport { useStore } from 'vuex';\nimport { computed } from 'vue';\nexport default {\n  name: 'AppHeaderNav',\n  setup() {\n    const store = useStore();\n    const list = computed(() => {\n      return store.state.category.list;\n    });\n    const show = (item) => {\n      store.commit()\n    }\n    return { list };\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n.app-header-nav {\n  width: 820px;\n  display: flex;\n  justify-content: space-around;\n  padding-left: 40px;\n  position: relative;\n  z-index: 998;\n  > li {\n    margin-right: 40px;\n    width: 38px;\n    text-align: center;\n    > a {\n      font-size: 16px;\n      line-height: 32px;\n      height: 32px;\n      display: inline-block;\n    }\n    &:hover {\n      > a {\n        color: @xtxColor;\n        border-bottom: 1px solid @xtxColor;\n      }\n      > .layer {\n        height: 132px;\n        opacity: 1;\n      }\n    }\n  }\n}\n.layer {\n  width: 1240px;\n  background-color: #fff;\n  position: absolute;\n  left: -200px;\n  top: 56px;\n  height: 0;\n  overflow: hidden;\n  opacity: 0;\n  box-shadow: 0 0 5px #ccc;\n  transition: all 0.2s 0.1s;\n  ul {\n    display: flex;\n    flex-wrap: wrap;\n    padding: 0 70px;\n    align-items: center;\n    height: 132px;\n    li {\n      width: 110px;\n      text-align: center;\n      img {\n        width: 60px;\n        height: 60px;\n      }\n      p {\n        padding-top: 10px;\n      }\n      &:hover {\n        p {\n          color: @xtxColor;\n        }\n      }\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}